<script defer="defer" type="text/javascript">
    (function () {
        var header = document.body.querySelector('.js-header');
        var headerMenuBtn = document.body.querySelector('.js-header-menu-btn');
        var headerLangBtns = document.body.querySelectorAll('.js-header-lang');

        // event for header onscroll behavior
        window.addEventListener('scroll', function () {
            checkScrollHeight(window.pageYOffset);
        });
        checkScrollHeight(window.pageYOffset);

        function checkScrollHeight(YOffset) {
            if (YOffset > header.getBoundingClientRect().height) {
                header.classList.add('header--fixed');
            } else {
                header.classList.remove('header--fixed');
            }
        }

        // click on languages buttons
        [].forEach.call(headerLangBtns, function (langBtn) {
            langBtn.addEventListener('click', function (e) {
                e.preventDefault();
                window.location.href = langBtn.dataset.src;
            })
        });
        // open menu
        headerMenuBtn.addEventListener('click', function (e) {
            if (headerMenuBtn.classList.contains('header__menu--opened')) {
                closeMenu();
            } else {
                window.scrollTo(0,0);
                openMenu(e);
            }
        });

        function openMenu(e) {
            e.stopPropagation();
            headerMenuBtn.classList.add('header__menu--opened');
            document.getElementById('secondary-content').classList.add('menu-opened');
            document.body.style.overflowY = 'hidden';

            window.addEventListener('click', menuStatus);
        }
        function closeMenu() {
            headerMenuBtn.classList.remove('header__menu--opened');
            document.getElementById('secondary-content').classList.remove('menu-opened');
            document.body.style.overflowY = 'initial';

            // document.body.querySelector('footer').classList.remove('state-1');

            window.removeEventListener('click', menuStatus);
        }
        function menuStatus(e) {
            var menuWrap = document.body.querySelector('.aside-menu__wrap');

            if (menuWrap.contains(e.target)) {
                // Clicked in box
            } else {
                // Clicked outside the box => close menu
                closeMenu();
            }
        }

    }());
</script>
