<script defer="defer" type="text/javascript">
    document.addEventListener('DOMContentLoaded', function () {
        (function () {
            var navItemsMobile = document.body.querySelectorAll('.js-welcome-nav-item-mobile');
            var navItemsMob = Array.prototype.slice.call(navItemsMobile, 0);
            var scrollBtn = document.body.querySelector('.js-scroll-btn');
            var quoteBtn = document.body.querySelector('.js-quote-btn');
            var serviceBtn = document.body.querySelector('.js-service-btn');
            var aboutSection = document.body.querySelector('.section__features');
            var contactSection = document.body.querySelector('.section__contacts');
            var videoBg = document.getElementById('welcome-video');

            videoBg.oncanplay = function () {
                console.log('Can start playing video');
                videoBg.muted = true;
            };
            // click on mobile nav items
            navItemsMob.forEach(function (navItem) {
                navItem.addEventListener('click', function () {
                    if (navItem.classList.contains('welcome__item--active')) {
                        window.location.href = navItem.dataset.src;
                    }
                    for (var i = 0; i < navItemsMob.length; i++) {
                        navItemsMob[i].classList.remove('welcome__item--active');
                    }
                    navItem.classList.add('welcome__item--active');
                })
            });
            // click on scroll button
            quoteBtn.addEventListener('click', function (e) {
                console.log(e, ' scroll to next section');
                e.preventDefault();
                scrollToElem({
                    elem: contactSection,
                    duration: 600,
                    timingFunc: null,
                    margin: 0
                });
            });
            serviceBtn.addEventListener('click', function (e) {
                console.log(e, ' scroll to next section');
                e.preventDefault();
                scrollToElem({
                    elem: contactSection,
                    duration: 600,
                    timingFunc: null,
                    margin: 0
                });
            });
            scrollBtn.addEventListener('click', function (e) {
                console.log(e, ' scroll to next section');
                e.preventDefault();
                scrollToElem({
                    elem: aboutSection,
                    duration: 600,
                    timingFunc: null,
                    margin: 0
                });
            });
            // event for onscroll behavior
            window.addEventListener('scroll', function () {
                checkScrollHeight(window.pageYOffset);
            });
            // check current window offsetY
            checkScrollHeight(window.pageYOffset);

            // helper functions
            function checkScrollHeight(YOffset) {
                if (YOffset > 5) {
                    scrollBtn.classList.add('scroll--hidden');
                } else {
                    scrollBtn.classList.remove('scroll--hidden');
                }
            }

            // functions that are responsible for scrolling with smooth animation
            function animation(options) {
                var start = Date.now();
                requestAnimationFrame(function animate() {
                    var timePassed = Date.now() - start,
                        timeFraction = timePassed / options.duration,
                        progress;
                    if (timeFraction > 1) timeFraction = 1;
                    progress = options.timing(timeFraction);
                    options.draw(progress);
                    if (timeFraction < 1) {
                        requestAnimationFrame(animate);
                    }
                });
            }

            function scrollToElem(options) {
                /* options = {
                              elem: NodeElem || Selector String,
                              duration: Number,
                              timingFunc: Function,
                              margin: number
                          }
                     */
                var elem = options.elem,
                    duration = options.duration || 1000,
                    timingFunc = options.timingFunc || swing,
                    margin = options.margin || 0;

                if (typeof elem === 'string') {
                    elem = document.body.querySelector(elem);
                }
                if (!elem) return;

                var elemPos = elem.getBoundingClientRect(),
                    startY = window.pageYOffset,
                    stopY = startY + elemPos.top,
                    way = stopY - startY - margin;

                animation({
                    duration: duration,
                    timing: timingFunc,
                    draw: function (progress) {
                        window.scrollTo(0, startY + progress * way);
                    }
                });

                function swing(t) {
                    return 0.5 - Math.cos(t * Math.PI) / 2;
                }

                function linear(time) {
                    return time;
                }

            }
        }());
    });

    (function () {
        window.requestAnimationFrame = window.requestAnimationFrame || window.mozRequestAnimationFrame ||
            window.webkitRequestAnimationFrame || window.msRequestAnimationFrame;
    }());

</script>
