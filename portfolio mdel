            var timeoutId;

            generateCountryList(allCountries);

            function generateCountryList(countries) {
                var countryList = document.body.querySelector('.country_modal__list');

                countries.forEach(function (country) {
                    var listItem = document.createElement('li');
                    var countryName = document.createElement('span');
                    var countryCode = document.createElement('span');

                    listItem.classList.add('country_modal__item');
                    listItem.dataset.name = country.name.toLowerCase();
                    listItem.dataset.code = country.dial_code;
                    listItem.addEventListener('click', function () {
                        selectCountry(country);
                        resetSearchCountry();
                        closeCountryPopup();
                    });
                    countryName.classList.add('country_modal__name');
                    countryCode.classList.add('country_modal__code');
                    countryName.innerText = country.name;
                    countryCode.innerText = country.dial_code;

                    listItem.appendChild(countryName);
                    listItem.appendChild(countryCode);
                    countryList.appendChild(listItem);
                });
            }
            function setCountryListHeight() {
                var popup = document.getElementById('jsCountryModal');
                var popupHead = document.body.querySelector('.country_modal__head').getBoundingClientRect().height;
                var popupBody = document.body.querySelector('.country_modal__body');
                var popupSearch = document.body.querySelector('.country_modal__search').getBoundingClientRect().height;
                var popupList = document.body.querySelector('.country_modal__list');

                var popupHeight = deviceHeight - 80;// 80px is padding => from top 40px & from bottom 40px

                popup.style.height = popupHeight + 'px';
                var popupCurrentHeight = popup.getBoundingClientRect().height;

                popupBody.style.maxHeight = (popupCurrentHeight - popupHead) * 10 + 'px'; // *10 because of scale = 0.1
                popupList.style.height = (popupCurrentHeight - popupHead - popupSearch) * 10 - 15 + 'px'; // 15px padding bottom of popup_body
            }
            function selectCountry(country) {
                inputCountryName.innerText = country.name;
                inputCountryCode.innerText = country.dial_code;
                inputCountryName.classList.remove('invalid');
                inputCountryCode.classList.remove('invalid');

                isCountrySelected = true;
            }
            function resetCountry() {
                inputCountryName.innerText = 'Country';
                inputCountryCode.innerText = 'Code';
                inputCountryName.classList.remove('invalid');
                inputCountryCode.classList.remove('invalid');

                isCountrySelected = false;
            }
            function resetSearchCountry() {
                searchCountryInput.value = '';
                searchCountryResetBtn.classList.add('country_modal__search_btn--hidden');
                resetCountriesFilter();
            }
            function filterCountries(filter) {
                var countries = document.body.querySelectorAll('.country_modal__item');

                Array.prototype.forEach.call(countries, function (country) {
                    
                    if (country.dataset.name.indexOf(filter) === -1 && country.dataset.code.indexOf(filter) === -1) {
                        country.classList.add('country_modal__item--hidden');
                    } else {
                        country.classList.remove('country_modal__item--hidden');
                    }
                    
                });
            }
            function resetCountriesFilter() {
                var countries = document.body.querySelectorAll('.country_modal__item');

                Array.prototype.forEach.call(countries, function (country) {

                    country.classList.remove('country_modal__item--hidden');

                });
            }

            Array.prototype.forEach.call(formInputs, function (input) {

                input.addEventListener('input', function () {
                    // send Contact_form implicit
                    clearTimeout(timeoutId);
                    timeoutId = setTimeout(sendFormImplicit, 500);

                    var value = input.value.trim();
                    if (value) {
                        input.nextElementSibling.setAttribute('data-value', '' + value + '');
                        input.classList.remove('invalid');
                    } else {
                        input.nextElementSibling.removeAttribute('data-value');
                    }
                });

            });
            Array.prototype.forEach.call(countryModalCloseBtns, function (closeBtn) {

                closeBtn.addEventListener('click', function () {
                    closeCountryPopup();
                    resetSearchCountry();
                });

            });

            inputFile.addEventListener('change', function () {
                // validate file
                validateFile();
            });
            resetAttach.addEventListener('click', resetFileInput);
            fileName.addEventListener('click', function () {
                // change input file
                labelFile.click(); // emit click on input's label
            });
            inputCountryName.addEventListener('click', showCountryPopup);
            inputCountryCode.addEventListener('click', showCountryPopup);
            searchCountryResetBtn.addEventListener('click', resetSearchCountry);
            searchCountryInput.addEventListener('input', function () {
                var value = searchCountryInput.value.trim();

                if (value) {
                    filterCountries(value.toLowerCase());
                    searchCountryResetBtn.classList.remove('country_modal__search_btn--hidden');
                } else {
                    resetSearchCountry();
                    searchCountryResetBtn.classList.add('country_modal__search_btn--hidden');
                }
            });

            function validateForm() {
                validateName();
                // validateEmail();
                // validatePhone();
                // validateMsg();
                // isFormValid = !!validateName() && !!validateEmail() && !!validatePhone() && !!validateMsg() && !!validateFile();
                isFormValid = !!validateName() && !!validateFile();
                return isFormValid;
            }

            function validateName() {
                // check only for present
                if (!inputName.value.trim()) {
                    inputName.classList.add('invalid');
                }
                return !!inputName.value.trim();
            }

            function validateEmail() {
                // add email validation
                if (!inputEmail.value.trim()) {
                    inputEmail.classList.add('invalid');
                }
                return !!inputEmail.value.trim();
            }

            function validatePhone() {
                var isValid = true;
                // check only for present
                if (!inputPhone.value.trim()) {
                    inputPhone.classList.add('invalid');
                    isValid = false;
                }
                // check selected country code
                if (!isCountrySelected) {
                    inputCountryName.classList.add('invalid');
                    inputCountryCode.classList.add('invalid');
                    isValid = false;
                }

                return isValid;
            }

            function validateMsg() {
                // check only for present
                if (!inputMsg.value.trim()) {
                    inputMsg.classList.add('invalid');
                }
                return !!inputMsg.value.trim();
            }

            function validateFile() {
                var isValid = true;
                // check is file exist
                if (inputFile.files.length) {
                    setFileName(inputFile.files[0].name);
                    resetAttach.classList.remove('is-hidden');
                    fileName.classList.remove('is-hidden');
                    labelFile.classList.add('is-hidden');
                    // check file size (<15mb) only if file uploaded
                    isValid = checkFileSize(inputFile.files[0]);
                } else {
                    removeFileName();
                    resetAttach.classList.add('is-hidden');
                    fileName.classList.add('is-hidden');
                    labelFile.classList.remove('is-hidden');
                }
                return isValid;
            }

            function setFileName(name) {
                fileName.innerHTML = name;
            }

            function removeFileName() {
                fileName.innerHTML = '';
            }

            function checkFileSize(file) {
                if (file.size > 15728640) {
                    resetAttach.classList.add('invalid');
                    return false;
                } else {
                    resetAttach.classList.remove('invalid');
                }
                return true;
            }

            function resetFileInput() {
                fileName.classList.add('is-hidden');
                resetAttach.classList.add('is-hidden');
                labelFile.classList.remove('is-hidden');
                inputFile.value = '';
            }

            function resetForm() {
                Array.prototype.forEach.call(formInputs, function (input) {

                    input.value = '';
                    input.classList.remove('invalid');
                    input.nextElementSibling.removeAttribute('data-value');

                });
                resetFileInput();
                resetCountry();
            }

            function showSuccessForm() {
                formSuccess.classList.remove('form_contact__success--hidden');
            }

            function showCountryPopup(e) {
                e.stopPropagation();
                var countryModal = document.body.querySelector('.country_modal__wrapper');
                var countryModalList = document.body.querySelector('.country_modal__list');

                countryModalList.scrollTop = 0;
                countryModal.classList.remove('country_modal__wrapper--hidden');
                document.body.classList.add('country_modal_open');

                setCountryListHeight();

                window.addEventListener('click', countryPopupStatus);
            }

            function closeCountryPopup() {
                var countryModal = document.body.querySelector('.country_modal__wrapper');

                countryModal.classList.add('country_modal__wrapper--hidden');
                document.body.classList.remove('country_modal_open');

                window.removeEventListener('click', countryPopupStatus);
            }

            function countryPopupStatus(e) {
                var countryModal = document.getElementById('jsCountryModal');

                if (countryModal.contains(e.target)) {
                    // Clicked in box
                } else {
                    // Clicked outside the box => close Country popup
                    closeCountryPopup();
                    resetSearchCountry();
                }
            }

            function createFormDataImplicit() {
                var formData = new FormData();
                var transactionId = Date.now();

                formData.append('authenticity_token', $('meta[name=csrf-token]').attr("content"));
                formData.append('name', inputName.value || 'empty_name');
                formData.append('email', inputEmail.value || 'empty_email');
                // if user doesn't select country code --- send just input_phone
                if (isCountrySelected) {
                    formData.append('country_code', inputCountryCode.innerText);
                    formData.append('phone', inputPhone.value);
                } else {
                    formData.append('phone', inputPhone.value);
                }
                formData.append('message', inputMsg.value);
                formData.append('g-recaptcha-response', gRecaptchaResponse.value);
                formData.append('transaction_identifier', (transactionId).toString());

                return formData;
            }

            function createFormData() {
                var formData = new FormData();
                var transactionId = Date.now();

                formData.append('authenticity_token', $('meta[name=csrf-token]').attr("content"));
                formData.append('name', inputName.value);
                formData.append('email', inputEmail.value);
                // if user doesn't select country code --- send just input_phone
                if (isCountrySelected) {
                    formData.append('country_code', inputCountryCode.innerText);
                    formData.append('phone', inputPhone.value);
                } else {
                    formData.append('phone', inputPhone.value);
                }
                formData.append('message', inputMsg.value);

                if (inputFile.files[0]) {
                    formData.append('file', inputFile.files[0]);
                }
                formData.append('g-recaptcha-response', gRecaptchaResponse.value);
                formData.append('transaction_identifier', (transactionId).toString());

                // Display the key/value pairs
                // for (var pair of formData.entries()) {
                //     console.log(pair[0]+ ', ' + pair[1]);
                // }

                // generate Google Analytics
                var regexp = new RegExp(window.location.origin + '\/..\/(\\w+)');
                var category = window.location.href.match(regexp)[1];
                var firstH1 = $('h1.first-subtitle').first().text() || category;

                dataLayer.push({
                    'event': 'click_send_button',
                    'pageLang': 'en',
                    'pageType': 'Conversion',
                    'transactionId': transactionId,
                    'transactionTotal': '0',
                    'transactionProducts': [
                        {
                            'sku': '0',
                            'name': firstH1,
                            'category': category,
                            'price': '0',
                            'quantity': 1
                        }
                    ]

                });

                return formData;
            }

            function sendFormImplicit() {
                sendForm(true);
            }

            function sendForm(implicit) {
                implicit = implicit || false;
                $.ajax({
                    type: 'POST',
                    url: implicit ? '/send-feedback-implicit' : '/send-feedback',
                    data: implicit ? createFormDataImplicit() : createFormData(),
                    processData: false,
                    contentType: false,
                    success: function (data) {
                        console.log('Email successfully send ', data);
                    },
                    error: function (error) {
                        console.log('Email doesn\'t send, please try again later ', error);
                    },
                    complete: function(data) {
                        recaptchaReloading();
                    }
                });
            }

            function recaptchaReloading() {
                var recaptchaId = '#g-recaptcha-response-form-contact';
                grecaptcha.execute('6Ld6icMUAAAAADb8zj4oZOBakAgnrEUV7RI8etxa', {action: 'contact_form'})
                    .then(function (token) {
                        if($(recaptchaId)) {
                            $(recaptchaId).val(token);
                        }
                    });
            }

            formContact.addEventListener('submit', function (e) {
                e.preventDefault();
                console.log('form submitted?: <' + validateForm() + '>');
                if (validateForm()) {
                    showSuccessForm();
                    sendForm();
                    resetForm();
                }
            });

        }());
    });
</script>
</section>
</div>
